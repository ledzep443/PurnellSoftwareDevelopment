<div class="page-header">
  <%= link_to projects_path, class: 'btn btn-default' do %>
    All Projects
  <% end %>
  <%= link_to edit_project_path(@project), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show project</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @project.name %></dd>
  <dt>Url:</dt>
  <dd><%= @project.url %></dd>
  <dt>Github:</dt>
  <dd><%= @project.github %></dd>
  <dt>Languages:</dt>
  <dd><%= @project.languages %></dd>
  <dt>Framework:</dt>
  <dd><%= @project.framework %></dd>
</dl>
